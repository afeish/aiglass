# AI Glass System - Environment Configuration
# Copy this file to .env and fill in your values

# Required: DashScope API key for ASR and Qwen-Omni
DASHSCOPE_API_KEY=your_dashscope_api_key_here

# Model paths (optional, defaults shown)
BLIND_PATH_MODEL=model/yolo-seg.pt
OBSTACLE_MODEL=model/yoloe-11l-seg.pt
YOLOE_MODEL_PATH=model/yoloe-11l-seg.pt

# Audio configuration
TTS_INTERVAL_SEC=1.0
ENABLE_TTS=true

# Logging
LOG_LEVEL=INFO

# Navigation parameters
AIGLASS_MASK_MIN_AREA=1500
AIGLASS_MASK_MORPH=3
AIGLASS_MASK_MISS_TTL=6
AIGLASS_PANEL_SCALE=0.65

# Port configuration
PORT=8081